{
  "schemaVersion": 1,
  "id": "rpimod",
  "version": "${version}",
  "name": "RPIMod",
  "description": "A tutorial mod for the Modding Basics Workshop",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/rpimod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.quatalog.rpimod.fabric.RPIModFabric"
    ],
    "client": [
      "com.quatalog.rpimod.fabric.client.RPIModFabricClient"
    ]
  },
  "mixins": [
    "rpimod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
